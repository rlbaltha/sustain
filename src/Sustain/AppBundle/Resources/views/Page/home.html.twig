{% extends '::base.html.twig' %}

{% block body -%}

    <div class="col-sm-12" style="background-color: #8edaf1; margin-top: -20px;margin-left: -15px; margin-right: -30px;">
        <img src="{{ asset('bundles/app/images/watershed_logo.jpg') }}" class="" alt="Watershed UGA logo">
    </div>
<div class="col-sm-12" style="padding:30px;">
    {% for entity in entities %}

        <div class="col-sm-4">
            <img src="{{ entity.teaserImage }}" class="img-responsive img-thumbnail" alt="{{ entity.menuName }}">
            {{ entity.teaser|raw }}
        </div>
    {% endfor %}
</div>

{% endblock %}
